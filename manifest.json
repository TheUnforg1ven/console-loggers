{
    "manifest_version": 2,
    "name": "Console Loggers",
    "description": "Adds ability to use btttv emotes in console",
    "version": "1.0.0",
    "icons": { "128": "images/icon_128.png" },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": "activeTab"
}